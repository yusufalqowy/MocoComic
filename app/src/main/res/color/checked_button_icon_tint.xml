<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Disabled state -->
    <item android:color="?attr/colorOnSurface" android:alpha="@dimen/material_emphasis_disabled" android:state_enabled="false" />
    <!-- Checkable, unchecked state -->
    <item android:color="?attr/colorOnPrimary" android:state_checkable="true" android:state_checked="false" />
    <!-- Default, Enabled state -->
    <item android:color="?attr/colorOnPrimary" />
</selector>